#include <sydio.h>

int main()

{

int ar[100];

int i;n; sum=0;

printf("Input the number of elements to be stored in the array:");

scanf("%d",&n);

printf("Input%d elements in the array;\n"n);

for(i=0;i<n;i++)

{printf("%d :",i);

scanf("%d",&ar[i]);

}

for(i=0;i<n;i++)

{

sun=sum+ar[i];

}

printf("Sum of all elements stored in the array is : %d\n\n",sum);

}

